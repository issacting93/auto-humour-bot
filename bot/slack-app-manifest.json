{
  "display_information": {
    "name": "HumourBot",
    "description": "Helping with the automation of meme generation",
    "background_color": "#0040ff"
  },
  "features": {
    "bot_user": {
      "display_name": "HumourBot",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/meme",
        "url": "https://auto-humour-bot.vercel.app/slack/events",
        "description": "Meme batch status and mark images used",
        "usage_hint": "status <batch_id> | used <batch_id> <image_id> [link]",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "commands",
        "chat:write"
      ]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": false
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
